{
  "name": "promptvalar-backend",
  "version": "1.0.0",
  "description": "PromptValar Backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "db:generate": "drizzle-kit generate:pg",
